body
	margin: auto
	margin-top: 15px;
	padding: 0px
	width: 1000px
	font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
	box-shadow: -10px 10px 25px #888888
	background-color: transparent
	border-radius: 10px
.title
	position: absolute
	z-index: 50
	padding-left: 10px
	color: #F5F5F5
	display: inline-block
	font-size: 24pt
.headertitle
	display: inline-block
	padding-top: 10px
	padding-left: 10px
	padding-right: 10px
	border-top-right-radius: 26px
	background-color: #6666DD
	width: 150px
	margin-top: 5px
	background: -webkit-gradient(linear, center top, center bottom, color-stop(0%, #738CB1), color-stop(100%, #4866DF))
	background: -moz-linear-gradient(top, #738CB1 0%, #4866DF 100%);
.headertitle a
	color: black
	text-decoration: none
.header
	height: 47px
	border-left-style: solid
	border-color: black
	border-width: 1px
	display: block
	padding: 0px
	border-top-left-radius: 10px
	border-top-right-radius: 10px
	background-color: black
	background: -webkit-gradient(linear, center bottom, center top, color-stop(0%, black), color-stop(100%, #333333))
	background: -moz-linear-gradient(top, #333333 0%, black 100%);
.content
	display: block
	padding: 0px
	background-color: whiteSmoke
	min-height: 300px
	border-left-style: solid
	border-right-style: solid
	border-color: black
	border-width: 1px
.footer
	display: block
	padding: 0px
	text-align: center
	color: whiteSmoke
	border-bottom-left-radius: 10px
	border-bottom-right-radius: 10px
	background-color: black
	background: -webkit-gradient(linear, center top, center bottom, color-stop(0%, black), color-stop(100%, #333333))
	background: -moz-linear-gradient(top, #333333 100%, black 0%);
	padding-top: 8px
	height: 24px
.menu
	display: block
	padding: 0px
	border-color: black
	border-width: 1px
	border-style: solid;
	background-color: whiteSmoke
	position: fixed
	top: 50px
	left: 50px
	height: 320px
	width: 180px
	border-radius: 10px
	background: -webkit-gradient(linear, center bottom, center top, color-stop(0%, black), color-stop(100%, black), color-stop(50%, #333333))
	background: -moz-linear-gradient(top, #333333 0%, black 100%);